{
	"name": "n8n-nodes-planning-center",
	"version": "0.2.0",
	"description": "n8n community nodes for Planning Center Online and Clearstream - church management and SMS messaging",
	"keywords": [
		"n8n-community-node-package",
		"planning-center",
		"clearstream",
		"church",
		"church-management",
		"pco",
		"ministry",
		"sms",
		"texting"
	],
	"license": "MIT",
	"homepage": "https://github.com/n8n-church/n8n-nodes-planning-center",
	"author": {
		"name": "n8n-church",
		"email": "contact@n8n-church.org"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/n8n-church/n8n-nodes-planning-center.git"
	},
	"engines": {
		"node": ">=18.10"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/PlanningCenterApi.credentials.js",
			"dist/credentials/PlanningCenterOAuth2Api.credentials.js",
			"dist/credentials/ClearstreamApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/PlanningCenter/PlanningCenterPeople.node.js",
			"dist/nodes/PlanningCenter/PlanningCenterServices.node.js",
			"dist/nodes/PlanningCenter/PlanningCenterCheckIns.node.js",
			"dist/nodes/PlanningCenter/PlanningCenterTrigger.node.js",
			"dist/nodes/Clearstream/Clearstream.node.js",
			"dist/nodes/Clearstream/ClearstreamTrigger.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^25.0.8",
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
